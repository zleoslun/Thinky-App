{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "app/**/*.ts",
    "app/**/*.tsx",
    "**/*.ts",
    "**/*.tsx",
    "node_modules/@types/**/*.d.ts",
    "env.d.ts"
  ]
}
